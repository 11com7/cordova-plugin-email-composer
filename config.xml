<?xml version='1.0' encoding='utf-8'?>
<widget id="de.appplant.cordova.plugin.emailcomposer.example" version="0.8.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EmailExample</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="windows-target-version" value="10.0" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-x-toast" spec="^2.7.0" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="osx" spec="^4.0.2" />
    <engine name="android" spec="^7.1.4" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="windows" spec="latest" />
</widget>
